<h1>Articles</h1>
<%= link_to 'New Article', new_tenant_article_path(@tenant) %>
<% if @articles.present? %>
  <ul>
    <% @articles.each do |article| %>
      <li>
        <%= link_to 'Show Article', tenant_article_path(@tenant, article) %><br>
        <%= link_to article.title, tenant_articles_path(@tenant, article) %> <br>
      </li>
    <% end %>
  </ul>
<% else %>
  <p>No articles available.</p>
<% end %>

